<section class="login-form">
<h1>User Signup</h1>

<form action="<%= users_url %>" method="POST">
  <%= csrf_token %>
  <label>Email
    <input type="text" name="user[email]">
  </label>
  <br>
  <label>Password
    <input type="password" name="user[password]">
  </label>
  <button class="form-button">Sign up!</button>
</form>
</section>
