<h1><%= collection.escape("name") %></h1>
<main class="collection-description">
  <%= collection.escape("description") %>
</main>
<ul class="collection-books">
	<% books.each(function(book) { %>
		<li>Title: <%= book.title %>, Author Email: <%= book.author %></li>
	<% }) %>
</ul>
<button id="edit-collection" class="form-button">Edit</button>
